<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bora.mapper.CardMapper">

	<select id="getCard" resultType="CardVO">
		select * from bora_card
		where card_num=#{card_num}
	</select>

	<select id="getCardList" resultType="CardVO">
		select * from bora_card
		where id=#{id}
	</select>
</mapper>